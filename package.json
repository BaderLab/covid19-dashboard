{
  "name": "covid19-dashboard",
  "version": "0.0.0",
  "description": "WIP",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxkfranz/covid19-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/maxkfranz/covid19-dashboard/issues"
  },
  "homepage": "https://github.com/maxkfranz/covid19-dashboard",
  "scripts": {
    "start": "http-server -p 3000",
    "bundle-profile": "cross-env NODE_ENV=production PROFILE=true webpack --profile",
    "lint:js": "eslint ./src",
    "bundle:js": "webpack",
    "watch:js": "webpack --watch",
    "watch:server": "http-server -p 3000",
    "watch:sync-bundle": "livereload \"build, src/views\" -w 1000",
    "lint:css": "stylelint src/**/*.css",
    "bundle:css": "postcss src/styles/index.css -o build/main.css",
    "watch:css": "postcss --verbose --watch src/styles/index.css -o build/main.css",
    "bundle:css-deps": "postcss src/styles/deps.css -o build/deps.css",
    "watch:css-deps": "postcss --watch src/styles/deps.css -o build/deps.css",
    "bundle": "run-s bundle:*",
    "build": "run-p bundle",
    "build-prod": "cross-env NODE_ENV=production run-s build",
    "clean": "rimraf build/*",
    "lint": "run-s lint:*",
    "watch": "run-p watch:*",
    "test:mocha": "mocha -r dotenv/config -r esm",
    "test:bundle": "run-s bundle",
    "test": "run-s lint test:*",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "dotenv": "^6.2.0",
    "eventemitter3": "^3.1.2",
    "fready": "^1.0.0",
    "http-server": "^0.12.1",
    "leaflet": "^1.6.0",
    "leaflet-providers": "^1.9.1",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hyperscript": "^3.2.0",
    "react-router-dom": "^5.1.2",
    "regenerator-runtime": "^0.13.5",
    "uuid": "^3.4.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "core-js": "^3.6.4",
    "cross-env": "^5.2.1",
    "cssnano": "^4.1.10",
    "dotenv-webpack": "^1.7.0",
    "d3-geo": "^1.11.9",
    "echo-cli": "^1.0.8",
    "eslint": "^6.8.0",
    "esm": "^3.2.25",
    "gh-pages": "^2.2.0",
    "livereload": "^0.9.1",
    "mocha": "^6.2.2",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.27",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^8.0.0",
    "rimraf": "^2.7.1",
    "stylelint": "^13.2.1",
    "stylelint-config-standard": "^20.0.0",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11"
  },
  "engines": {
    "node": ">=10"
  },
  "browserslist": "> 0.5%, last 2 versions, Firefox ESR, not dead"
}
